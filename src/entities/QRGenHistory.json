{
  "name": "QRGenHistory",
  "type": "object",
  "properties": {
    "code_id": {
      "type": "string",
      "description": "Unique identifier for the QR code"
    },
    "payload": {
      "type": "string",
      "description": "The data/URL encoded in the QR code"
    },
    "payload_sha256": {
      "type": "string",
      "description": "SHA-256 hash of the payload"
    },
    "size": {
      "type": "number",
      "description": "QR code size in pixels"
    },
    "creator_id": {
      "type": "string",
      "description": "Email or ID of the user who created the code"
    },
    "status": {
      "type": "string",
      "enum": [
        "safe",
        "suspicious",
        "blocked"
      ],
      "default": "safe"
    },
    "type": {
      "type": "string",
      "enum": [
        "url",
        "text",
        "email",
        "phone",
        "sms",
        "wifi"
      ],
      "default": "url"
    },
    "image_format": {
      "type": "string",
      "enum": [
        "png",
        "svg",
        "jpg"
      ],
      "default": "png"
    },
    "error_correction": {
      "type": "string",
      "enum": [
        "L",
        "M",
        "Q",
        "H"
      ],
      "default": "M",
      "description": "Error correction level: L(7%), M(15%), Q(25%), H(30%)"
    },
    "foreground_color": {
      "type": "string",
      "default": "#000000",
      "description": "QR code foreground color (hex)"
    },
    "background_color": {
      "type": "string",
      "default": "#FFFFFF",
      "description": "QR code background color (hex)"
    },
    "has_logo": {
      "type": "boolean",
      "default": false,
      "description": "Whether a custom logo is embedded"
    },
    "logo_url": {
      "type": "string",
      "description": "URL of the embedded logo image"
    }
  },
  "required": [
    "code_id",
    "payload",
    "creator_id"
  ]
}